{
	"name": "@wsu/web-design-system",
	"version": "0.1.1",
	"description": "Web Design System for Washington State University.",
	"private": true,
	"main": "src/index.js",
	"devDependencies": {
		"@babel/core": "^7.8.3",
		"@babel/polyfill": "^7.8.3",
		"@babel/preset-env": "^7.8.3",
		"@wsu/build-tools": "git+https://github.com/washingtonstateuniversity/wsu-build-tools.git",
		"autoprefixer": "^9.7.4",
		"babel-loader": "^8.0.6",
		"css-loader": "^3.4.2",
		"fibers": "^3.1.0",
		"file-loader": "^4.3.0",
		"html-loader": "^1.1.0",
		"html-webpack-plugin": "^4.0.4",
		"mini-css-extract-plugin": "^0.8.2",
		"mitt": "^1.2.0",
		"modularscale-sass": "^3.0.8",
		"node-sass": "^4.13.1",
		"objectFitPolyfill": "^2.3.0",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"postcss-loader": "^3.0.0",
		"rimraf": "^3.0.2",
		"sass": "^1.3.0",
		"sass-loader": "^8.0.2",
		"webpack": "^4.41.5",
		"webpack-cli": "^3.3.10",
		"webpack-dev-server": "^3.10.1"
	},
	"scripts": {
		"start": "npm run start:demo & npm run start:bundles & npm run start:components",
		"start:bundles": "webpack --env.NODE_ENV=development --silent --watch --config bundles.config.js",
		"start:components": "webpack --env.NODE_ENV=development --silent --watch --config components.config.js",
		"start:demo": "webpack-dev-server --env.NODE_ENV=development --silent --watch --config demo.config.js",
		"build": "npm run clean && npm run build:components && npm run build:bundles && npm run build:demo",
		"build:bundles": "webpack --env.NODE_ENV=production --config bundles.config.js",
		"build:components": "webpack --env.NODE_ENV=production --config components.config.js",
		"build:demo": "webpack --env.NODE_ENV=production --config demo.config.js",
		"clean": "rimraf ./components/dist & rimraf ./builds/dist & rimraf ./demo/dist"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/washingtonstateuniversity/wsu-web-design-system.git"
	},
	"author": "WSU University Communications",
	"license": "GPL-2.0",
	"bugs": {
		"url": "https://github.com/washingtonstateuniversity/wsu-web-design-system/issues"
	},
	"homepage": "https://github.com/washingtonstateuniversity/wsu-web-design-system#readme",
	"browserslist": [
		"last 2 versions",
		"Firefox ESR",
		"android 4",
		"opera 12",
		"ie >= 11",
		"ie_mob >= 11",
		"ff >= 30",
		"chrome >= 34",
		"safari >= 7",
		"opera >= 23",
		"ios >= 7",
		"android >= 4.4",
		"bb >= 10",
		"not dead"
	]
}
